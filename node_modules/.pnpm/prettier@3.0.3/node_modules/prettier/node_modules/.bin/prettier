#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/bin/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/bin/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules/prettier/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/prettier@3.0.3/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../../bin/prettier.cjs" "$@"
fi
