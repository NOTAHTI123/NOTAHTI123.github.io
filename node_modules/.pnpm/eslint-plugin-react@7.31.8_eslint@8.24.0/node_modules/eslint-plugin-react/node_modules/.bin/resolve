#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/bin/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/bin/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules:/media/exton/0AC29AEBC29AD9F1/VS_code_programs/todo-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@2.0.0-next.4/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@2.0.0-next.4/node_modules/resolve/bin/resolve" "$@"
fi
